using terms from application "Terminal"
	tell application "Terminal"
		activate
		
		-- open the terminal's window, set its dimensions		
		set tab1 to do script
		set currentWindow to front window
		set position of currentWindow to {1180, 0}
		set number of rows of tab1 to 45
		-- set number of columns of tab1 to 90
		set number of columns of tab1 to 80
		
		do script " " in tab1
	end tell
end using terms from
